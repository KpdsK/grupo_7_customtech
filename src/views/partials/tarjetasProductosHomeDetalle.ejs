<a class="sub-main" href="/products/<%= producto.id %>">
    <i class="fa-regular fa-heart" onclick=<% if(locals.isLoged) { %>"agregarProductoAWishList(<%= producto.id %>, <%= locals.userLog.id %>)<%} else {%> irALogin() <%} %>" ></i>
    <img class=productos src="/images/<%= producto.image %>" alt="Imagen principal">
    <div class="sub-main-descripcion">
    <p class="descripciones"><strong><%= producto.name %></strong></p>
    <p class="precios">$<%= producto.price %></p>
    </div>
</a>