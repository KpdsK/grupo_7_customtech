                <section class="tarjetas-productos">
                    <article class="tarjeta-producto tar-prod-uno">
                        <i class="fa fa-trash tacho-basura" onclick="quitarProductoCarrito('tar-prod-uno')"
                            aria-hidden="true"></i>
                        <div class="tarjeta-producto-cabecera">
                            <div class="tarjeta-producto-cabecera-imagen">
                                <img src="images/img-macbook-pro-2019.jpg" alt="imagen-producto-1">
                            </div>
                            <div class="tarjeta-producto-titulo-descripcion">
                                <legend>MacBook Pro blabla</legend>
                                <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Unde, delectus?</p>
                            </div>
                        </div>
