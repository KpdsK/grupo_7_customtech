                                    <div class="tarjeta-producto-detalle-item">
                                        <label>Color</label>
                                        <div class="caracteristica">
                                            <i class="fa fa-circle" aria-hidden="true"></i>
                                            <label>Negro</label>
                                        </div>
                                    </div>
                                    <div class="tarjeta-producto-detalle-item">
                                        <label class="cantidad">Cantidad</label>
                                        <div class="contador-cantidad">
                                            <input type="button" id="contador-cantidad-menos-1"
                                                onclick="restar('contador-cantidad-input-1')" value="-">
                                            <input name="cantidad" type="text" id="contador-cantidad-input-1" value="1">
                                            <input type="button" id="contador-cantidad-mas-1"
                                                onclick="sumar('contador-cantidad-input-1')" value="+">
                                        </div>
                                    </div>
                                    <div class="tarjeta-producto-detalle-item">
                                        <label>Precio</label>
